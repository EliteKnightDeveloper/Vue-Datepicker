{
  "name": "vue3-date-time-picker",
  "version": "2.3.2",
  "description": "Datepicker component for Vue 3",
  "author": "Vuepic",
  "private": false,
  "license": "MIT",
  "homepage": "https://vue3datepicker.com",
  "main": "dist/vue3-date-time-picker.ssr.js",
  "browser": "dist/vue3-date-time-picker.esm.js",
  "module": "dist/vue3-date-time-picker.esm.js",
  "unpkg": "dist/vue3-date-time-picker.min.js",
  "files": [
    "dist/*",
    "src/**/*",
    "index.d.ts"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.ts",
    "build": "run-s clean build:rollup build:css",
    "clean": "rimraf ./dist",
    "build:rollup": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife",
    "build:css": "cross-env NODE_ENV=production sass src/Vue3DatePicker/style/main.scss dist/main.css --style compressed",
    "test": "cross-env NODE_ENV=test vue-cli-service test:unit"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^21.0.1",
    "@rollup/plugin-node-resolve": "^13.0.6",
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^5.2.0",
    "@typescript-eslint/parser": "^5.2.0",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.2.20",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^8.0.0",
    "@vue/test-utils": "^2.0.0-0",
    "core-js": "^3.19.0",
    "cross-env": "^7.0.3",
    "date-fns": "^2.25.0",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-vue": "^7.20.0",
    "minimist": "^1.2.5",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.11",
    "postcss-scss": "^4.0.1",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.3",
    "rollup-plugin-postcss": "^4.0.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^6.0.0",
    "sass-loader": "^10.1.1",
    "stylelint": "^13.13.1",
    "stylelint-config-prettier": "^9.0.2",
    "stylelint-config-recommended": "^5.0.0",
    "stylelint-prettier": "^1.2.0",
    "stylelint-scss": "^3.21.0",
    "typescript": "^4.4.4",
    "vue": "^3.2.20",
    "vue-eslint-parser": "^8.0.0",
    "vue-jest": "^5.0.0-0"
  },
  "peerDependencies": {
    "vue": ">=3.2.0"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Vuepic/vue3-date-time-picker.git"
  },
  "types": "index.d.ts",
  "keywords": [
    "calendar",
    "vue",
    "vue3-datepicker",
    "vue-datepicker",
    "datepicker",
    "datetimepicker",
    "daterangepicker",
    "timepicker"
  ]
}
